{
  "expo": {
    "name": "Smart Finances",
    "slug": "smart-finances",
    "runtimeVersion": "2.4.3(38)",
    "version": "2.4.4",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "backgroundColor": "#020027",
    "primaryColor": "#FF5200",
    "platforms": ["ios", "android"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#020027"
    },
    "android": {
      "package": "com.vap.smartfinances",
      "versionCode": 38,
      "permissions": [
        "VIBRATE",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "USE_FINGERPRINT",
        "USE_BIOMETRIC"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "smartfinances.com.br", // Seu dom√≠nio
              "pathPrefix": "/oauth-native-callback" // Path para o Clerk redirecionar
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.vap.smartfinances",
      "buildNumber": "1",
      "supportsTablet": true
    },
    "extra": {
      "eas": {
        "projectId": "a17a404b-a307-4781-a880-7b717ea58c58"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/a17a404b-a307-4781-a880-7b717ea58c58"
    },
    "owner": "va-p",
    "plugins": [
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#020027",
          "image": "./assets/icon.png",
          "dark": {
            "image": "./assets/icon.png",
            "backgroundColor": "#020027"
          }
        }
      ]
    ]
  }
}

{
  "expo": {
    "name": "Smart Finances",
    "slug": "smart-finances",
    "owner": "va-p",
    "version": "2.21.3",
    "scheme": "com.vap.smartfinances",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "backgroundColor": "#E6E9F4",
    "primaryColor": "#FF5200",
    "platforms": ["ios", "android"],
    "android": {
      "package": "com.vap.smartfinances",
      "versionCode": 89,
      "googleServicesFile": "./android/app/google-services.json",
      "permissions": [
        "VIBRATE",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "USE_FINGERPRINT",
        "USE_BIOMETRIC"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "smartfinances.com.br",
              "pathPrefix": "/oauth-native-callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.vap.smartfinances",
      "buildNumber": "1",
      "supportsTablet": true,
      "backgroundColor": "#E6E9F4",
      "icon": "src/assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a17a404b-a307-4781-a880-7b717ea58c58"
      }
    },
    "submit": {
      "production": {
        "android": {
          "serviceAccountKeyPath": "./android/app/google-services.json",
          "track": "internal"
        }
      }
    },
    "updates": {
      "enabled": false
    },
    "plugins": [
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics",
      "expo-font",
      ["expo-splash-screen"],
      "expo-web-browser"
    ]
  }
}

{
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "npx expo start --dev-client",
    "revopush:login": "REVOPUSH_ACCESS_KEY=${REVOPUSH_ACCESS_KEY:-$CP_ACCESS_KEY} revopush login --accessKey \"$REVOPUSH_ACCESS_KEY\"",
    "revopush:whoami": "revopush whoami",
    "revopush:deploys": "revopush deployment list \"SmartFinances\"",
    "revopush:release:android:staging": "TBV=${TBV:-2.23.0} revopush release-react \"SmartFinances\" android -d Staging --target-binary-version $TBV",
    "revopush:release:android:prod": "TBV=${TBV:-2.23.0} revopush release-react \"SmartFinances\" android -d Production --target-binary-version $TBV",
    "revopush:release:ios:staging": "TBV=${TBV:-2.23.0} revopush release-react \"SmartFinances iOS\" ios -d Staging --target-binary-version $TBV",
    "revopush:release:ios:prod": "TBV=${TBV:-2.23.0} revopush release-react \"SmartFinances iOS\" ios -d Production --target-binary-version $TBV",
    "revopush:promote:stg2prod": "TBV=${TBV:-2.23.0} revopush promote \"SmartFinances\" Staging Production --target-binary-version $TBV",
    "revopush:history:android:staging": "revopush deployment history \"SmartFinances\" Staging --platform android",
    "revopush:history:android:prod": "revopush deployment history \"SmartFinances\" Production --platform android",
    "revopush:history:ios:staging": "revopush deployment history \"SmartFinances\" Staging --platform ios",
    "revopush:history:ios:prod": "revopush deployment history \"SmartFinances\" Production --platform ios",
    "revopush:rollback:android": "revopush rollback \"SmartFinances\" Production --platform android",
    "revopush:rollback:ios": "revopush rollback \"SmartFinances\" Production --platform ios",
    "clean-android": "cd android && ./gradlew clean",
    "prebuild-android": "npx expo prebuild --platform android --no-install",
    "build-android-dev": "cd android && ./gradlew assembleDebug",
    "build-android-release": "cd android && ./gradlew bundleRelease",
    "build-android-dev-eas": "eas build --profile development --platform android",
    "build-android-release-eas": "eas build --profile production --platform android",
    "clean-ios": "cd ios && xcodebuild clean -workspace smartfinances.xcworkspace -scheme smartfinances",
    "prebuild-ios": "npx expo prebuild --platform ios --no-install",
    "build-ios-dev": "cd ios && xcodebuild build -workspace smartfinances.xcworkspace -scheme smartfinances -configuration Debug -sdk iphonesimulator -derivedDataPath build",
    "build-ios-release": "cd ios && xcodebuild archive -workspace smartfinances.xcworkspace -scheme smartfinances -configuration Release -sdk iphoneos -archivePath build/smartfinances.xcarchive",
    "build-ios-dev-eas": "eas build --profile development --platform ios",
    "build-ios-release-eas": "eas build --profile production --platform ios",
    "bundle-analyze": "EXPO_UNSTABLE_ATLAS=true npx expo start --dev-client",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "patch-package"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.6.9",

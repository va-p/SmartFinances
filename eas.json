{
  "cli": {
    "version": ">= 5.3.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "environment": "development",
      "distribution": "internal",
      "channel": "development",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "environment": "preview",
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "environment": "production",
      "channel": "production",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
